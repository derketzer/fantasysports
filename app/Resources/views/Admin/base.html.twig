<!DOCTYPE html>
<!--[if lt IE 7]> <html class="lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--><html lang="en"><!--<![endif]-->
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="Katana Studios">

        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/bootstrap/css/bootstrap.min.css" media="all">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/css/jquery-ui.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/jquery-ui.custom.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/timepicker/jquery-ui-timepicker.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/uniform/css/uniform.default.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/ibutton/jquery.ibutton.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/custom-plugins/circular-stat/circular-stat.css">
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.6.1/fullcalendar.min.css" media="screen">
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.6.1/fullcalendar.print.css" media="print">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/css/fonts/icomoon/style.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/css/main-style.css" media="screen">
        <link rel="stylesheet" href="https://s3.amazonaws.com/fantasysports.mx/admin-assets/custom-plugins/picklist/picklist.css" media="screen">
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

        {% if stylesToLoad is defined %}
            {% for style in stylesToLoad %}
                <link href="{{ asset("bundles/fantasysportsadmin/css/#{style}") }}" rel="stylesheet" type="text/css"/>
            {% endfor %}
        {% endif %}

        <title>El Villano Chelero</title>
    </head>

    <body data-show-sidebar-toggle-button="false" data-fixed-sidebar="true">
        <div id="wrapper">
            {{ include('::Admin/Includes/header.html.twig') }}
            <div id="content-wrap">
                <div id="content">
                    <div id="content-outer">
                        <div id="content-inner">
                            {{ include('::Admin/Includes/sidebar.html.twig') }}
                            <div id="sidebar-separator"></div>
                            <section id="main" class="clearfix">
                                <div id="main-header" class="page-header">
                                    <ul class="breadcrumb">
                                        {% block breadcrumbs %}{% endblock %}
                                    </ul>
                                    {% if pageTitle is defined %}
                                        <h1 id="main-heading">
                                            {{ pageTitle }}
                                            {% if pageDescription is defined %}
                                                <span>{{ pageDescription }}</span>
                                            {% endif %}
                                        </h1>
                                    {% endif %}
                                </div>
                                <div id="main-content">
                                    {% block body %}{% endblock %}
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
            </div>
            {{ include('::Admin/Includes/footer.html.twig') }}
        </div>

        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/libs/jquery-1.8.3.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/bootstrap/js/bootstrap.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/libs/jquery.placeholder.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/libs/jquery.mousewheel.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/template.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/setup.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/customizer.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/uniform/jquery.uniform.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/js/jquery-ui-1.9.2.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/jquery-ui.custom.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/timepicker/jquery-ui-timepicker.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/jui/jquery.ui.touch-punch.min.js"></script>
        <!--[if lt IE 9]>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/js/libs/excanvas.min.js"></script>
        <![endif]-->
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/flot/jquery.flot.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/flot/plugins/jquery.flot.tooltip.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/flot/plugins/jquery.flot.pie.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/flot/plugins/jquery.flot.resize.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/custom-plugins/circular-stat/circular-stat.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/sparkline/jquery.sparkline.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/ibutton/jquery.ibutton.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.12.0/moment.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.6.1/fullcalendar.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.6.1/lang/es.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/datatables/TableTools/js/TableTools.min.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/plugins/datatables/dataTables.bootstrap.js"></script>
        <script src="https://s3.amazonaws.com/fantasysports.mx/admin-assets/custom-plugins/picklist/picklist.min.js"></script>

        {% if scriptsToLoad is defined %}
            {% for script in scriptsToLoad %}
                <script type="text/javascript" src="{{ asset("bundles/fantasysportsadmin/js/#{script}") }}"></script>
            {% endfor %}
        {% endif %}

        {% block javascript %}{% endblock %}

        {% if app.environment != 'dev' %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-51385617-1', 'auto');
            ga('send', 'pageview');
        </script>
        {% endif %}

        <div id="fb-root"></div>
    </body>
</html>